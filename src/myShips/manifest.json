{
  "name": "__MSG_extention_name__",
  "description": "__MSG_extention_description__",
  "default_locale": "en",
  "version": "1.5",
  "manifest_version": 3,
  "icons": {
    "128": "icon_ship.png"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://warehouse.korabli.su/*",
        "https://korabli.su/*"
      ],
      "include_globs": [
        "https://warehouse.korabli.su/*",
        "https://korabli.su/?*/content/contents-and-drop-rates-of-containers/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "https://warehouse.korabli.su/api/user_data/",
        "ships.json"
      ],
      "matches": [
        "https://warehouse.korabli.su/*",
        "https://korabli.su/*"
      ]
    }
  ]
}
